### RFC config file

base	= /tmp/rfcs
baseurl	= file:///tmp

pager	= cat
user-agent = Lynx/2.8.8dev.16 libwww-FM/2.14 SSL-MM/1.4.1 OpenSSL/1.0.1e-fips
timeout	= 60

[src]
index   = $baseurl/rfc-index.txt
txt	= $baseurl/rfc%04d.txt

[cache]
dir	= $base
out     = $base/rfc%04d.txt
index   = $base/rfc-index.txt

### That's all, folks!
